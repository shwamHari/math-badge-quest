{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "add_quest"
      ],
      "properties": {
        "add_quest": {
          "type": "object",
          "required": [
            "id",
            "operation"
          ],
          "properties": {
            "id": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "operation": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "submit_solution"
      ],
      "properties": {
        "submit_solution": {
          "type": "object",
          "required": [
            "quest_id",
            "solution",
            "sub_question_index"
          ],
          "properties": {
            "quest_id": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "solution": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "sub_question_index": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
